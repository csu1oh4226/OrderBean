    "studio": "prisma studio",
    "seed": "tsx src/scripts/seed.ts",
    "seed:test": "NODE_ENV=test tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
